{
  "name": "@timsexperiments/theme",
  "displayName": "@timsexperiments/theme",
  "description": "Editor theme to match the Tim's Experiments color palette",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timsexperiments/timsexperiments-theme.git"
  },
  "bugs": "https://github.com/timsexperiments/timsexperiments-theme/issues",
  "keywords": [
    "textmate"
  ],
  "exports": {
    "./*": "./themes/*"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Tim's Experiments Light",
        "uiTheme": "vs",
        "path": "./themes/tim-s-experiments-light-color-theme.json"
      },
      {
        "label": "Tim's Experiments Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/tim-s-experiments-dark-color-theme.json"
      }
    ]
  }
}
